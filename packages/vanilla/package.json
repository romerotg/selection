        "selecting",
        "selection",
        "user-selection",
        "user-interface",
        "ui-library",
        "ui"
    ],
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "vite": "^5.3.4",
        "vite-plugin-banner": "^0.7.1",
        "vite-plugin-dts": "^4.0.0-beta.1"
    }
}
